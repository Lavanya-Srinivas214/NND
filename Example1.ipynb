      "source": [
        "import numpy as np\n",
        "\n",
        "x = np.array([[1, 0, 1]])\n",
        "\n",
        "w1 = np.array([\n",
        "    [0.5, 0.4],\n",
        "    [-0.2, 0.1],\n",
        "    [0.3, -0.5]\n",
        "])\n",
        "\n",
        "w2 = np.array([[0.7],\n",
        "               [0.2]])\n",
        "\n",
        "hidden = x.dot(w1)\n",
        "z = hidden.dot(w2)\n",
        "\n",
        "output = 1 / (1 + np.exp(-z))\n",
        "\n",
        "print(hidden)\n",
        "print(output)"
      ]
    }
